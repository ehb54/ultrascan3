{
  "version": 4,
  "cmakeMinimumRequired": { "major": 3, "minor": 21 },
  "configurePresets": [
    {
      "name": "base",
      "hidden": true,
      "binaryDir": "${sourceDir}/build/${presetName}",
      "generator": "Ninja",
      "cacheVariables": {
        "CMAKE_CXX_STANDARD": "17",
        "CMAKE_EXPORT_COMPILE_COMMANDS": "ON",
        "CMAKE_TOOLCHAIN_FILE": "${sourceDir}/vcpkg/scripts/buildsystems/vcpkg.cmake",
        "VCPKG_OVERLAY_TRIPLETS": "${sourceDir}/admin/cmake/triplets"
      },
      "environment": {
        "VCPKG_DISABLE_METRICS": "1"
      }
    },
    {
      "name": "base-qt5-qwt616",
      "hidden": true,
      "inherits": "base",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "VCPKG_MANIFEST_FEATURES": "qt5"
      }
    },
    {
      "name": "base-qt6",
      "hidden": true,
      "inherits": "base",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "VCPKG_MANIFEST_FEATURES": "qt6",
        "USE_QT6": "ON"
      }
    },
    {
      "name": "base-qt5-qwt630",
      "hidden": true,
      "inherits": "base",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "VCPKG_MANIFEST_FEATURES": "qt5-qwt630"
      }
    },
    {
      "name": "debug-base",
      "hidden": true,
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Debug",
        "VCPKG_BUILD_TYPE": "debug"
      }
    },
    {
      "name": "release-base",
      "hidden": true,
      "binaryDir": "${sourceDir}/build/${presetName}",
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "VCPKG_BUILD_TYPE": "release"
      }
    },
    {
      "name": "macos-base",
      "hidden": true,
      "cacheVariables": {
        "VCPKG_TARGET_TRIPLET": "x64-osx-dynamic",
        "CMAKE_OSX_DEPLOYMENT_TARGET": "11.0"
      }
    },
    {
      "name": "linux-base",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_POSITION_INDEPENDENT_CODE": "ON",
        "VCPKG_TARGET_TRIPLET": "x64-linux-dynamic"
      }
    },
    {
      "name": "windows-base",
      "hidden": true
    },
    {
      "name": "mingw-base",
      "hidden": true,
      "cacheVariables": {
        "VCPKG_TARGET_TRIPLET": "x64-mingw-dynamic",
        "VCPKG_HOST_TRIPLET": "x64-mingw-dynamic",
        "VCPKG_CXX_FLAGS": "-std=gnu++17 -fpermissive",
        "VCPKG_C_FLAGS": "-std=gnu17 -fpermissive"
      }
    },
    {
      "name": "macos-release-qt5-qwt616",
      "displayName": "macOS Release Qt5 Qwt6.1.6",
      "inherits": ["release-base", "macos-base", "base-qt5-qwt616"],
      "condition": { "type": "equals", "lhs": "${hostSystemName}", "rhs": "Darwin" }
    },
